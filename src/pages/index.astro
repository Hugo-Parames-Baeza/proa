---
import Layout from "../layouts/Layout.astro";
import Nav from "../components/header/Nav.astro";
import Mensaje from "../components/Mensaje.astro";
import Logo from "../components/header/Logo.astro";
import Somos from "../components/sections/Somos.astro";
import Novedades from "../components/sections/Novedades.astro";
import Equipo from "../components/sections/Equipo.astro";
import Footer from "../components/footer/Footer.astro";
import Redes from "../components/footer/Redes.astro";

